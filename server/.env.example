# Database
DATABASE_URL="postgresql://i4ops:password@localhost:5432/i4ops_dashboard"

# SSH Configuration for VM monitoring and log collection
SSH_USERNAME=i4ops
SSH_PASSWORD=your_ssh_password_here
SSH_TIMEOUT=30

# u0 Log Collection Settings
U0_HOST=u0
U0_IP=100.76.195.14
LOG_SYNC_INTERVAL=60  # seconds (60 = every minute)
LOG_BASE_PATH=/mnt/vm-security

# Tailscale OAuth (optional)
TS_OAUTH_CLIENT_ID=your_client_id
TS_OAUTH_CLIENT_SECRET=your_client_secret
TAILNET=your_tailnet.ts.net

# Supabase (optional)
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Application Settings
NODE_ENV=development
PORT=4000
LOG_LEVEL=info 